{
  "name": "mycli3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:dll": "webpack -p --progress --config ./webpack.dll.conf.js",
    "build": "vue-cli-service build --progress --color --display-modules",
    "lint": "vue-cli-service lint",
    "start": "pm2 start pm2.json --env production;",
    "start:qa": "pm2 start pm2.json --env qa;",
    "start:rdtest": "pm2 start pm2.json --env rdtest;",
    "start:server": "export NODE_ENV=development; nodemon ./server/app"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^2.6.5",
    "glob": "^7.1.4",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-helmet": "^4.2.0",
    "koa-router": "^7.4.0",
    "koa-static-cache": "^5.1.2",
    "koa2-connect-history-api-fallback": "^0.1.2",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "node-sass": "^4.14.1",
    "qrcode": "^1.4.4",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.10",
    "vue-cropperjs": "^4.1.0",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^3.9.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "autodll-webpack-plugin": "^0.4.2",
    "babel-eslint": "^10.0.1",
    "element-ui": "^2.13.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "mockjs": "^1.0.1-beta3",
    "preload-webpack-plugin": "^2.3.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-awesome": "^3.5.4",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "webpack-cli": "^3.3.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
